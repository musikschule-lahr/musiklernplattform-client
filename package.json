{
  "name": "de.musikschulelahr.audioplattform",
  "displayName": "audioplattform",
  "version": "0.0.1",
  "main": "index.js",
  "author": "St√§dtische Musikschule Lahr",
  "license": "Apache-2.0",
  "scripts": {
    "start": "cordova prepare && cordova build browser -- --webpackConfig webpack.config.js && cordova run browser ",
    "start-reload": "cordova prepare && cordova build browser -- --webpackConfig webpack.config.js && cordova run browser -- --livereload ",
    "start:android-reload": "cordova prepare && cordova build android -- --webpackConfig webpack.config.js && cordova run android -- --livereload",
    "start:android": "cordova prepare && cordova build android -- --webpackConfig webpack.config.js && cordova run android",
    "start:ios": "cordova prepare && cordova build ios -- --webpackConfig webpack.config.js && cordova run ios",
    "start:ios-reload": "cordova prepare && cordova build ios -- --webpackConfig webpack.config.js && cordova run ios -- --livereload",
    "init": "cordova prepare",
    "eslint:updateSchema": "graphql-codegen",
    "readd:browser": "cordova platform remove browser && cordova platform add browser",
    "readd:ios": "cordova platform remove ios && cordova platform add ios",
    "readd:android": "cordova platform remove android && cordova platform add android"
  },
