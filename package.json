    "eslint-import-resolver-webpack": "^0.12.1",
    "fast-sort": "^2.2.0",
    "graphql": "^15.3.0",
    "keycloak-js": "^9.0.3",
    "matrix-js-sdk": "^8.4.1",
    "moment": "^2.25.3",
