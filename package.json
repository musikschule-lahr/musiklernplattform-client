    "cordova-ios": "^6.1.1",
    "cordova-plugin-qrscanner": "3.0.0",
    "cordova-plugin-webpack": "^1.0.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "css-loader": "^3.5.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
